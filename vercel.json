{
    "version":2,
    "builds": [
        {
          "src": "myproject/wsgi.py",
          "use": "@vercel/python"
        },
        {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "staticfiles_build"
          }
        }
    ],
    "routes": [
        {
          "src": "/static/(.*)",
          "dest": "static/$1"
        },
        {
          "src": "/(.*)",
          "dest": "myproject/wsgi.py"
        }
    ]
}